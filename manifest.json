{
    "name": "Sphinx",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "sphinx.html",
      "default_icon": "logo.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens sphinx.html",
        "permissions":[
          "activeTab",
          "declarativeContent"
        ]
      }
    }
  }